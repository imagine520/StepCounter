{
  "name": "IntelligentAssistant",
  "version": "0.1.0",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "macpackager": "electron-packager . IntelligentAssistant --platform=darwin --arch=x64 --out ../macDeploy --electronVersion 1.7.5 --overwrite --asar",
    "winpackager": "electron-packager . IntelligentAssistant  --platform=win32 --arch=x64 --out ../winDeploy  --electronVersion 1.7.5  --overwrite --asar",
    "win32packager": "electron-packager . IntelligentAssistant --platform=win32 --arch=ia32 --out ../winDelpoy  --electronVersion 1.7.5  --overwrite --asar",
    "packager": "electron-packager . IntelligentAssistant --all --out ../linuxDeploy  --electronVersion 1.7.5  --overwrite --asar"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "egoonet",
  "license": "egoonet",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "electron-is": "^2.3.0",
    "jquery": "^3.2.1",
    "nconf": "^0.8.4"
  }
}
